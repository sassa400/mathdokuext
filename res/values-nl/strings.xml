    <!-- The title and summary of the "Maximum games in elapsed time chart" option. This option restricts the number of games which will be displayed in an elapsed time chart. -->
    <string name="statistics_setting_elapsed_time_chart_maximum_games_title">Aantal puzzels in grafiek speeltijd</string>
